---
export const prerender = false;
import { getViewsOfWiki } from "../lib/web";

interface Props {
	title: string;
}
const { title } = Astro.props;
const views = await getViewsOfWiki(title);
---

{
	views !== null && views.views !== null && views?.views >= 0 && (
		<p class="text-zinc-500">
			This page has been viewed {views} times in recent 60 days
		</p>
	)
}
