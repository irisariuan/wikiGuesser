---
export const prerender = false;
interface Props {
	isDailyChallenges: boolean;
}
import DailyChallengesSelector from "./DailyChallengesSelector";
import AllChallengesSelector from "./AllChallengesSelector";
const { isDailyChallenges } = Astro.props;
const url = Astro.url;
---

{
	isDailyChallenges ? (
		<DailyChallengesSelector url={url} client:load />
	) : (
		<AllChallengesSelector url={url} client:load />
	)
}
